<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

    <servlet>
        <servlet-name>RegisterUIServlet</servlet-name>
        <servlet-class>seu.automation.web.UI.RegisterUIServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterUIServlet</servlet-name>
        <url-pattern>/RegisterUIServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>seu.automation.web.RegisterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/RegisterServlet</url-pattern>
    </servlet-mapping>

    <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>seu.automation.web.LoginServlet</servlet-class>
</servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UserUpdateUIServlet</servlet-name>
        <servlet-class>seu.automation.web.UI.UserUpdateUIServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserUpdateUIServlet</servlet-name>
        <url-pattern>/UserUpdateUIServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UserUpdateServlet</servlet-name>
        <servlet-class>seu.automation.web.UserUpdateServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserUpdateServlet</servlet-name>
        <url-pattern>/UserUpdateServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DisplayStateServlet</servlet-name>
        <servlet-class>seu.automation.web.DisplayStateServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DisplayStateServlet</servlet-name>
        <url-pattern>/DisplayStateServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>MainServlet</servlet-name>
        <servlet-class>seu.automation.web.MainServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MainServlet</servlet-name>
        <url-pattern>/MainServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>UserDisplayServlet</servlet-name>
        <servlet-class>seu.automation.web.UserDisplayServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserDisplayServlet</servlet-name>
        <url-pattern>/UserDisplayServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>RoleDisplayServlet</servlet-name>
        <servlet-class>seu.automation.web.RoleDisplayServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RoleDisplayServlet</servlet-name>
        <url-pattern>/RoleDisplayServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ElevatorUserDisplayServlet</servlet-name>
        <servlet-class>seu.automation.web.ElevatorUserDisplayServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ElevatorUserDisplayServlet</servlet-name>
        <url-pattern>/ElevatorUserDisplayServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ElevatorProducerDisplayServlet</servlet-name>
        <servlet-class>seu.automation.web.ElevatorProducerDisplayServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ElevatorProducerDisplayServlet</servlet-name>
        <url-pattern>/ElevatorProducerDisplayServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>MapUIServlet</servlet-name>
        <servlet-class>seu.automation.web.UI.MapUIServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MapUIServlet</servlet-name>
        <url-pattern>/MapUIServlet</url-pattern>
    </servlet-mapping>

    <filter>
        <filter-name>CharacterEncodingFilter</filter-name>
        <filter-class>seu.automation.filter.CharacterEncodingFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>CharacterEncodingFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
</web-app>